<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huang.dao.RoleMapper">

    <!--查-->

    <select id="queryAllRole" resultType="Role">
        select * from role
    </select>

    <!--查询类型-->
    <select id="queryRoleByType" resultType="Role">
        select * from role where userType=#{userType}
    </select>


</mapper>